@model List<string>
@{
    ViewData["Title"] = "Data List";
}

<div class="text-center">
    <h1 class="display-4">Welcome @ViewData["Title"]</h1>
    <p>Learn about <a href="https://learn.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>

<div>
    <table>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Income</th>
        </tr>
        @for (int i = 0; i < 10; i++){
            <tr>
                <td>@i.ToString()</td>
                <td>@Model[i]</td>
                <td></td>
            </tr>
        }
    </table>
</div>
